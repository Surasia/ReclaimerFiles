<plugin>
	<_field_struct name="AnyTag">
		<_field_int64_integer name="vtable space" />
		<_field_struct name="internal struct">
			<_field_long_integer name="global tag ID" />
			<_field_pad name="local tag handle" length="4" />
			</_field_struct>
		</_field_struct>
	<_field_block_64 name="Marker Groups">
		<_field_string_id name="Group" />
		<_field_block_64 name="Markers">
			<_field_string_id name="Visual region name" />
			<_field_string_id name="Name" />
			<_field_string_id name="Parent" />
			<_field_real_point_3d name="Position" />
			<_field_real_euler_angles_3d name="Rotation" />
			<_field_real_vector_3d name="Scale" />
			<_field_real_vector_3d name="Direction" />
			</_field_block_64>
		</_field_block_64>
	<_field_block_64 name="Hint Marker Groups">
		<_field_long_enum name="Hint Type">
			<Item Name="Bunker" />
			<Item Name="Corner Left" />
			<Item Name="Corner Right" />
			<Item Name="Vault Step" />
			<Item Name="Vault Crouch" />
			<Item Name="Hoist Stand" />
			<Item Name="Hoist Crouch" />
		</_field_long_enum>
		<_field_block_64 name="Hint Markers">
			<_field_string_id name="Visual region name" />
			<_field_string_id name="Name" />
			<_field_string_id name="Parent" />
			<_field_real_point_3d name="Position" />
			<_field_real_euler_angles_3d name="Rotation" />
			<_field_real_vector_3d name="Scale" />
			<_field_real name="Length" />
			</_field_block_64>
		</_field_block_64>
	<_field_block_64 name="Pathfinding Markers">
		<_field_string_id name="Name" />
		<_field_string_id name="Parent" />
		<_field_real_point_3d name="Position" />
		<_field_real name="Radius" />
		<_field_long_flags name="Flags">
			<Item Name="Vehicle Only" />
			<Item Name="Remains When Open" />
		</_field_long_flags>
		</_field_block_64>
	<_field_block_64 name="Target Marker Groups">
		<_field_string_id name="Group" />
		<_field_block_64 name="Target Markers">
			<_field_string_id name="Visual region name" />
			<_field_string_id name="Name" />
			<_field_string_id name="Parent" />
			<_field_real_point_3d name="Position" />
			<_field_real name="Radius" />
			</_field_block_64>
		</_field_block_64>
	</plugin>