<plugin>
	<_field_struct name="AnyTag">
		<_field_int64_integer name="vtable space" />
		<_field_struct name="internal struct">
			<_field_long_integer name="global tag ID" />
			<_field_pad name="local tag handle" length="4" />
			</_field_struct>
		</_field_struct>
	<_field_long_flags name="flags">
		<Item Name="Doesn't use wind" />
		<Item Name="Uses grid attach top" />
	</_field_long_flags>
	<_field_string_id name="marker attachment name" />
	<_field_string_id name="secondary axis attachment name" />
	<_field_tag_reference_64 name="Shader" />
	<_field_explanation name="Default cloth vertices" />
	<_field_short_integer name="grid x dimension" />
	<_field_short_integer name="grid y dimension" />
	<_field_real name="grid spacing x" />
	<_field_real name="grid spacing y" />
	<_field_block_64 name="collision spheres">
		<_field_string_id name="object marker name" />
		<_field_real name="radius" />
		</_field_block_64>
	<_field_explanation name="Cloth Properties" />
	<_field_struct name="properties">
		<_field_short_enum name="Integration type">
			<Item Name="verlet" />
		</_field_short_enum>
		<_field_short_integer name="Number iterations" />
		<_field_real name="weight" />
		<_field_real name="drag" />
		<_field_real name="wind_scale" />
		<_field_real name="wind_flappiness_scale" />
		<_field_real name="longest_rod" />
		</_field_struct>
	<_field_explanation name="Import or grid data" />
	<_field_block_64 name="vertices">
		<_field_real_point_3d name="initial position" />
		<_field_real_vector_2d name="uv" />
		</_field_block_64>
	<_field_block_64 name="indices">
		<_field_short_integer name="index" />
		</_field_block_64>
	<_field_block_64 name="strip indices">
		<_field_short_integer name="index" />
		</_field_block_64>
	<_field_block_64 name="links">
		<_field_real name="default_distance" />
		<_field_long_integer name="index1" />
		<_field_long_integer name="index2" />
		<_field_pad name="pad" length="4" />
		</_field_block_64>
	<_field_int64_integer name="m_pipelineStates" />
	</plugin>